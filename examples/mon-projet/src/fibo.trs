function fibonacci(n: int32): int32 {
    if (n <= 1) {
        return n;
    }

    var a: int32 = 0;
    var b: int32 = 1;

    for (var i: int32 = 2; i <= n; i = i + 1) {
        val next: int32 = a + b;
        a = b;
        b = next;
    }

    return b;
}

function main() {
    console.write("=== Fibonacci Demo ===");

    for (var n: int32 = 0; n <= 15; n = n + 1) {
        console.write(`fib(${n}) = ${fibonacci(n)}`);
    }
}
