import { BASE, add } from "./math";
import { Point, makePoint } from "./point";

function main() {
    var p: Point = makePoint(1, 2);
    val total: int32 = add(p.sum(), BASE);

    console.write(total);
    console.write(p.moveX(5));
    console.write(string(boolean(BASE > 0 and p.sum() > 0)));
}
