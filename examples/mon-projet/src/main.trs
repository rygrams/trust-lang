import { Instant } from "std/time";

function fibonacci(n: number32): number32 {
    if (n <= 1) {
        return n;
    }
    return fibonacci(n - 1) + fibonacci(n - 2);
}

function main() {
    let start0 = Instant.now();
    let r0 = fibonacci(0);
    let e0 = start0.elapsed().as_millis();
    console.log(`fibonacci(0) = ${r0} [${e0}ms]`);

    let start1 = Instant.now();
    let r1 = fibonacci(1);
    let e1 = start1.elapsed().as_millis();
    console.log(`fibonacci(1) = ${r1} [${e1}ms]`);

    let start5 = Instant.now();
    let r5 = fibonacci(5);
    let e5 = start5.elapsed().as_millis();
    console.log(`fibonacci(5) = ${r5} [${e5}ms]`);

    let start40 = Instant.now();
    let r40 = fibonacci(40);
    let e40 = start40.elapsed().as_millis();
    console.log(`fibonacci(40) = ${r40} [${e40}ms]`);
}
